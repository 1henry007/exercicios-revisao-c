#include <stdio.h>

int main() {
    int i, j;
    int soma = 0;

    
    for (i = 0; i <= 6; i++) {
        for (j = i; j <= 6; j++) {
            
            if (i == 4) {
                soma += 4; 
            }
            if (j == 4 && i != j) {
                soma += 4; 
            }
        }
    }

    
    printf("A soma total dos numeros 4 no domino Ã©: %d\n", soma);

    return 0;
